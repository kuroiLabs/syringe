{
  "name": "@kuroi/syringe",
  "version": "1.0.1",
  "description": "TypeScript dependency injection framework",
  "main": "index.js",
  "scripts": {
    "build": "shx rm -Rf dist && tsc && shx cp package.json dist/package.json",
    "pack": "npm pack ./dist",
    "start:publish": "npm publish --access=public --scope=@kuroi",
    "test:publish": "npm publish --access=public --scope=@kuroi --dry-run",
    "test": "ts-node tests/test.ts",
    "test:isolate": "ts-node tests/isolated-test.ts"
  },
  "keywords": [
    "di",
    "dependency",
    "injection"
  ],
  "author": "kuro <kuro@kuroi.io>",
  "license": "ISC",
  "devDependencies": {
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "typescript": "^3.5.1"
  }
}
